
<div class="list-title">{{list.title}}</div>

<div class="task-container">
  {{#each list.tasks as |task| }}
  <div class="task-title">
    <p>{{task.title}}</p>
  </div>
  {{/each}}

 <div class="add-task">
   {{input placeholder="Add New Task" value=newTaskTitle enter="handleAddTask" name="addTask"}}
  </div>
</div>

